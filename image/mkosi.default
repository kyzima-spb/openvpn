[Distribution]
Distribution=debian
#Release=bookworm
Release=trixie

[Output]
Format=tar
Output=openvpn.tar.xz
Bootable=no

[Validation]
CheckSum=true

[Packages]
WithNetwork=true
Packages=curl
         easy-rsa
         gettext-base
         iproute2
         less
         nftables
         nano
         openvpn
         procps    # /proc file system utilities
         zip
         unattended-upgrades  # automatic installation of security upgrades
